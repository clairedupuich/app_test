		{
			"name": "Update Profile",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"response Json\", function(){\r",
							"    pm.response.to.be.json;\r",
							"})\r",
							"//Vérifier que la réponse HTTP renvoie 200\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"//Vérifier que le message de la réponse est \"User profile updated successfully\"\r",
							"pm.test(\"Response message is correct\", function () {\r",
							"    pm.expect(pm.response.json().message).to.eql(\"User profile updated successfully\");\r",
							"});\r",
							"\r",
							"const body = pm.response.json();\r",
							"\r",
							"if(pm.response.code === 401){\r",
							"    pm.test(\"token non fourni\",function(){\r",
							"        pm.expect(body.message).to.eql(\"Token non fourni\");\r",
							"    })\r",
							"} else if (pm.response.code === 201) {\r",
							"    pm.test(\"la valeur de la cle est correct\",function(){\r",
							"        pm.expect(body.message).to.eql(\"User profile updated successfully\");\r",
							"    });\r",
							"};"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
