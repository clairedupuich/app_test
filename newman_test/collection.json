							"});\r",
							"//Vérifier que la réponse est au format JSON.\r",
							"pm.test(\"Response is JSON\", function () {  \r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"//3 Vérifier que le corps de la réponse contient les clés 'msg' et 'otp'.\r",
							"pm.test(\"Response contains 'msg' and 'otp'\", function () {\r",
							"    pm.expect(pm.response.json()).to.have.property('msg');\r",
